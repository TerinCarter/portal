<h1>Sign up</h1>
 
<%= form_for(@user) do |f| %>

<%= render 'shared/error_messages' %>

  <div class="field">
    <%= f.label :nickname %> 
    <%= f.text_field :nickname %> 
  </div>
 
  <div class="field">
    <%= f.label :email %> 
    <%= f.text_field :email %> 
  </div>
 
  <div class="field">
    <%= f.label :password %> 
    <%= f.text_field :password %> 
  </div>
 
  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>

<% end %>

<%= link_to 'Back', users_path %>
