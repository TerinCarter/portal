<p id="notice"><%= notice %></p>

<h1>Listing Typesoftwares</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @typesoftwares.each do |typesoftware| %>
      <tr>
        <td><%= typesoftware.name %></td>
        <td><%= link_to 'Show', typesoftware %></td>
        <td><%= link_to 'Edit', edit_typesoftware_path(typesoftware) %></td>
        <td><%= link_to 'Destroy', typesoftware, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Typesoftware', new_typesoftware_path %>
